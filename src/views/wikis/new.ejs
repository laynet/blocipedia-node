<!DOCTYPE html>
<html>
  <head>
    <% include ../static/partials/head.ejs %>
  </head>
  <body>
    <main class="container">
      <% include ../static/partials/navbar.ejs %>

      <h1>New Wikis</h1>

      <form method="POST" action="/wikis/create">
        <div class="form-group">
          <label>Wiki Title</label>
          <input
            type="text"
            name="title"
            placeholder="Enter Title"
            class="form-control"
          />
          <small class="text-muted" id="titleHelp"
            >title must be at least 8 characters in length.</small
          >
        </div>
        
        <div class="form-group">
          <label>Wiki Body</label>
          <textarea rows="4" class="form-control" name="body" placeholder="Enter Body"></textarea>
          <small class="text-muted" id="bodyHelp"
          >Body must be at least 10 characters in length.</small
        >
    </div>
          
        <button type="submit" class="btn btn-primary" />Submit</button>
      </form>
    </main>
    <% include ../static/partials/baseScripts.ejs %>
</body>
</html>
   
  
  